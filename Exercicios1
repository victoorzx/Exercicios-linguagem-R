#ex1
seq(1,5,1)
seq(5,1,-1)
seq(1,20,5)
#seq(20,1,5)
seq(20,1,-5)

#ex2
rep(6,8)
rep(c(2,3),5)
rep(c(2,3,5),5)
c(rep(2,4), rep(3,5))
c(rep(1,5), rep(2,3), rep(3,5))

#ex3
a<-5:10
a
a[3]
a[1:4]
a[c(1,4)]
a[a>7]
a[a<7]
a[c(-2,-3)]
#ex4
b<-seq(1,11,2)
b
b<-matrix(b,3,2,1)
b
b[1,2]
summary(b)
#ex5
c<-matrix(1:20,ncol=4)
c
d<-cbind(c,4:8)
d
d<-rbind(d,(1:5))
d[3,2]
d[,3]
d[3,]
#ex6
x<-array(1:16,c(4,2,2))
x
x[2,2,2]
